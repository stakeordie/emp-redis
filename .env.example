# Example environment configuration for EmProps Redis Worker
# Copy this file to .env and modify as needed

# Redis Hub connection
REDIS_API_HOST=localhost
REDIS_API_PORT=8001

# Worker identification
WORKER_ID=worker-1

# Authentication
# IMPORTANT: This must match the token set on the Redis Hub
WEBSOCKET_AUTH_TOKEN=your-secure-token-here

# Worker settings
HEARTBEAT_INTERVAL=20

# Logging
LOG_LEVEL=INFO
